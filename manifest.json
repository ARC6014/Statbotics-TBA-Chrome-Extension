{
  "name": "Statbotics Inside TBA",
  "description": "This extension brings scouting statistics from Statbotics into TBA (The Blue Alliance). Developed by Yusuf Ege DoÄŸan of ARC#6014",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "home.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.thebluealliance.com/team/*"],
      "js": ["data.js"]
    },
    {
      "matches": ["https://www.thebluealliance.com/event/*"],
      "js": ["data.js"]
    },
    {
      "matches": ["https://www.thebluealliance.com/match/*"],
      "js": ["data.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "arcbotics_logo.png",
    "32": "arcbotics_logo.png",
    "48": "arcbotics_logo.png",
    "128": "arcbotics_logo.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "ThisIsFakeID@example.com",
      "data_collection_permissions": {
        "required": ["none"],
        "optional": []
      }
    }
  }
}
